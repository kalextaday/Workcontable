@model List<WebApplication2.Models.TableViewModels.ComparativoTableViewModel>
@{ 
    int index = 1;
}
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <thead>
        <tr>
            <th rowspan="2" valign="middle">#</th>
            <th rowspan="2" valign="middle">Fecha Inicio</th>
            <th rowspan="2" valign="middle">Fecha Fin</th>
            <th colspan="3" align="right">Presupuesto</th>
            <th rowspan="2">Varianza</th>
            <th colspan="3" align="center">Libro Contable</th>
        </tr>
        <tr>
            <th>Id</th>
            <th>Valor Presupuesto</th>
            <th>Total Gastos</th>
            <th>Id</th>
            <th>Total Ingresos</th>
            <th>Total Gastos</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var oElemento in Model)
        {
            <tr>
                <td>@index</td>
                <td>@oElemento.fecha_inicio_pre</td>
                <td>@oElemento.fecha_fin_pre</td>
                <td>@oElemento.id_presupuesto</td>
                <td>@oElemento.valor_pre</td>
                <td>@oElemento.total_gastos_pre</td>
                <td>@oElemento.varianza</td>
                <td>@oElemento.id_lib_contable</td>
                <td>@oElemento.total_ingresos</td>
                <td>@oElemento.total_gastos_lib</td>
            </tr>
            index++;
        }
        
    </tbody>
</table>
